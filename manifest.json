{
	"name": "Edrania",
	"version": "1",
	"description": "Edrania plugin.",
	"manifest_version": 2,
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
				"https://edrania.net/*"
			],
			"js": [
				"/js/vendor/jquery-3.5.1.min.js",
				"/js/edrania.js"
			],
			"css": [
				"/css/edrania.css"
			]
		}
	]

}