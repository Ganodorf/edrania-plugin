{
	"name": "Edrania",
	"version": "1.1.1",
	"description": "Edrania plugin.",
	"manifest_version": 2,
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"https://edrania.net/*"
			],
			"js": [
				"/js/vendor/jquery-3.5.1.min.js",
				"/js/prefill.js",
				"/js/hoverInfo.js",
				"/js/auction.js",
				"/js/teamGame.js",
				"/js/challenges.js",
				"/js/tavern.js",
				"/js/edrania.js"
			],
			"css": [
				"/css/edrania.css"
			]
		}
	],
	"browser_action": {
		"default_popup": "popup.html"
	}
}