{
	"name": "Edrania",
	"version": "1.11.0",
	"description": "Edrania plugin.",
	"manifest_version": 2,
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"https://edrania.net/*",
				"https://www.edrania.net/*"
			],
			"js": [
				"/js/vendor/jquery-3.5.1.min.js",
				"/js/prefill.js",
				"/js/hoverInfo.js",
				"/js/persistentFilter.js",
				"/js/auction.js",
				"/js/vendor.js",
				"/js/teamGame.js",
				"/js/challenges.js",
				"/js/challenge.js",
				"/js/tournament.js",
				"/js/playerStatus.js",
				"/js/duelReport.js",
				"/js/clanMine.js",
				"/js/profile.js",
				"/js/attributes.js",
				"/js/tavern.js",
				"/js/talentCalculator.js",
				"/js/talentCalculator/orc.js",
				"/js/talentCalculator/troll.js",
				"/js/talentCalculator/human.js",
				"/js/talentCalculator/dwarf.js",
				"/js/talentCalculator/elf.js",
				"/js/observer.js",
				"/js/workDistrict.js",
				"/js/workshop.js",
				"/js/searchGladiator.js",
				"/js/edrania.js"
			],
			"css": [
				"/css/edrania.css"
			]
		}
	],
	"browser_action": {
		"default_popup": "popup.html"
	}
}
